  task Task1 {
    subworkflow Workflow2;
  }
  task Task2 {
    implementation "file://implementation_A.py";
    param value = 0.2;
  }
  task Task3 {
    implementation "file://implementation_B.py";
  }
  task Task4 {
    subworkflow SubWorkflow;
